<!--Forward_kinematics demo-->
<launch> 	
  <arg name="robot_name"                default="sgr532"/>
  <!--sagittarius_moveit-->
  <include file="$(find sagittarius_moveit)/launch/demo_true.launch">
      <arg name="robot_name"    value="$(arg robot_name)"/>
  </include>

  <!--drow start node-->
  <node pkg="sagittarius_drawstar_and_display" type="moveit_eye_in_hand.py" name="moveit_eye_in_hand" ns="$(arg robot_name)">
  </node>


  <!--start script-->
  <node pkg="sagittarius_drawstar_and_display" type="new_terminal_eye_in_hand.sh" name="send_topic">
  </node>



  <!-- *** -->
  <!-- realsense node -->
  <!-- <include file="$(find realsense2_camera)/launch/rs_camera.launch"/>

  <include file="$(find aruco_ros)/launch/single.launch"/>
 -->

</launch>
